#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long a;
    while(1)
    {
        cin>>a;
        if(a==1999)
        {
          cout<<"Correct"<<endl;
          break;
        }
        else cout<<"Wrong"<<endl;
    }
}
